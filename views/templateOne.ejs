                <% if (editing) { %>
                    <p class="promotion"><a href="/edit-template/<%= template._id %>?edit=true">Edit your WebSailer</a></p>
                <% } else { %>
                    <p class="promotion">Made with WebSailer</p>
                <% } %>
            </div>
        </div>

    </div>
</body>
</html>